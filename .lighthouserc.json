{
  "ci": {
    "collect": {
      "numberOfRuns": 1
    },
    "assert": {
      "preset": "lighthouse:recommended",
      "assertions": {
        "uses-responsive-images": "off",
        "heading-order": "off",
        "unsized-images": "off",
        "uses-rel-preconnect": "off",
        "link-name": "off",
        "aria-valid-attr-value": "off",
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}
